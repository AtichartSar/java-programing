
                        JSONObject jsonObject = new JSONObject(mJsonData.toString());
                        LOG.info("jsonObject"+jsonObject);
                        JSONArray remindes = jsonObject.getJSONArray("reminders");
                        Gson gson = new Gson();
                        for (int i = 0; i < remindes.length(); i++) {
                            LOG.info("remindes>>>"+remindes.get(i));
                            TrxReminder trx = gson.fromJson(remindes.get(i).toString(), TrxReminder.class);
                            LOG.info("trx>>>"+trx+" i "+i);
                        }
                       
